{
  "name": "sync-offline-actions",
  "version": "2.2.0",
  "description": "Library to synchronize actions with backend when the connection is restored",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "compile": "babel -d lib/ src/ --ignore '**/test.js'",
    "prepublishOnly": "npm run compile",
